{
  "name": "__MSG_extName__",
  "version": "0.9.4",
  "description": "__MSG_extDesc__",
  "icons": { 
	"128": "res/icons/Deezer_icon_128x128.png",
	"16": "res/icons/Deezer_icon_16x16.png",
	"32": "res/icons/Deezer_icon_32x32.png",
	"48": "res/icons/Deezer_icon_48x48.png"
	},
  "browser_action": {
    "default_icon": "res/icons/Deezer_icon_32x32.png",
    "default_title": "Deezer+",
